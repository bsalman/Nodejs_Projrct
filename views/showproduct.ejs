        let productObj={
            name:,
            description:,
            imgs:[],
            categories:,
            price:,
            size:,
            color:,
        }
        document.querySelector('#addToCardBtn').addEventListener('click',e=>{
            e.preventDefault();
            let productsArr =[]
        })
    </script> -->
</body>

</html>